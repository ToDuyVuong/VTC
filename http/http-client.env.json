{
  "dev": {
    "host": "localhost",
    "port": 3000,
    "proxy": {
      "/api": {
        "target": "http://localhost:8181",
        "changeOrigin": true,
        "pathRewrite": {
          "^/api": ""
        }
      }
    },
    "autoOpenBrowser": true,
    "errorOverlay": true,
    "notifyOnErrors": true,
    "poll": false,
    "useEslint": true,
    "showEslintErrorsInOverlay": false,
    "devtool": "cheap-module-eval-source-map",
    "cacheBusting": true,
    "cssSourceMap": true,
    "cssModules": false,
    "cssLoaderOptions": {
      "localIdentName": "[name]-[local]-[hash:base64:5]",
      "camelCase": true
    }
  }
}